msgstr "si (?P<item_name>.+) en sala o inventario"

#: server/architext/verbs/checks.py:31
msgid "there is an item called {item_name} in this room"
msgstr "hay un objeto llamado {item_name} en esta sala"

#: server/architext/verbs/checks.py:34
msgid "there is an item called {item_name} in your inventory"
msgstr "hay un objeto llamado {item_name} en tu inventario"

